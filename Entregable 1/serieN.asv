function [nError] = serieN(error)
format long e;
n=1;
er = 1;
while error < er
    atan = 4*(6*arcotangente(n,1/8) + 2*arcotangente(n,1/57) + arcotangente(n,1/239));
    er = abs(pi - atan);
    if error > er
        format short;
        nError[1] = n;
        format long e;
        
        nError[2] = e;
        break
    end
    n = n+1;
end
end